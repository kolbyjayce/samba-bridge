{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "noImplicitThis": false,
  },
  "include": ["src/**/*", "review/SMBMessages"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
